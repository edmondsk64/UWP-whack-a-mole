<Window x:Class="_3080.StartWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_3080"
        mc:Ignorable="d"
        Title="StartWindow" Height="350" Width="525">
    <Viewbox>
        <Grid Height="900" Width="1600">
            <TextBox x:Name="startText" HorizontalAlignment="Left" Height="204" IsEnabled="False" IsReadOnly="True" FontSize="150" TextWrapping="Wrap" Text="Click Here To Start" VerticalAlignment="Top" Width="1253" Margin="172,341,0,0" BorderThickness="0"/>
            <Button x:Name="startButton" Click="startButton_Click" Opacity="0" Content="Button" HorizontalAlignment="Left" VerticalAlignment="Top" Width="1600" Height="900"/>
        </Grid>
    </Viewbox>
</Window>
